#include<stdio.h>
int main(){

    long long int m,n, fm =1, fn = 1 ,i,j;
    while(scanf("%lld %lld",&m,&n)!=EOF){

        for(i=1;i<=m; i++){
            fm = fm*i;
        }
        for(j=1; j<=n; j++){
            fn = fn *j;
        }
        printf("%lld\n",fm+fn);
        fm =1;
        fn =1;
    }

    return 0;
}
